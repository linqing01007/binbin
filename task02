<html>
	<head>
		<meta charset = "utf-8">
		<title>IFE JavaScript Task 01</title>
		<style type = "text/css">
			ul {
				list-style:none;
			}
		</style>
	</head>
	<body>
		<h3>aqi-list</h3>
		<ul id = "aqi-list">

		</ul>
		<script type = "text/JavaScript">
			var aqiData = [
				["beijing", 90], ["shanghai", 50], ["fuzhou", 10], ["guangzhou", 50], ["chengdu", 90], ["xian", 100]
			];
			(function() {
				for (var i = 0; i < aqiData.length; i++) {
					console.log(aqiData[i])
					if (aqiData[i][1] >= 80) {
						var ul = document.getElementById("aqi-list");
						var li = document.createElement("li");
						var text = aqiData[i][0] + ":" + aqiData[i][1]; 
						var textNode = document.createTextNode(text);
						li.appendChild(textNode);
						ul.appendChild(li);
					}
				}
			})();
		</script>
	</body>
</html>
